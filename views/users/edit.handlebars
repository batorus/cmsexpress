  
{{title}}
{{#each results}}
    <li class="list-group-item">

    <form action="/app/users/update/{{this.id}}" method="POST" class="mb-4" novalidate>
           <div class="form-group">
             <label for="name">Name</label>
             <input type="text" name="name" class="form-control" value="{{this.name}}">
           </div>

           <div class="form-group">
             <label for="email">Email</label>
             <input type="email" name="email" class="form-control" value="{{this.email}}">
           </div>
        <input type="submit" value="Save" class="btn btn-primary ">

     </form>

        <a href="/app/users/index">Cancel</a>

   </li>
  {{/each}}
